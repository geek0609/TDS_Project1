{
  "version": 2,
  "builds": [
    {
      "src": "virtual_ta.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "virtual_ta.py"
    },
    {
      "src": "/(.*)",
      "dest": "virtual_ta.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "FLASK_APP": "virtual_ta.py"
  }
} 