# Environment Variables for TDS Virtual TA
# Copy this file to .env and fill in your actual API keys

# Google Gemini API Key (required for LLM responses)
GEMINI_API_KEY=your_gemini_api_key_here

# AI Pipe Token (for PromptFoo evaluations)
AIPIPE_TOKEN=your_aipipe_token_here

# OpenAI settings for PromptFoo (uses AI Pipe)
OPENAI_API_KEY=${AIPIPE_TOKEN}
OPENAI_BASE_URL=https://aipipe.org/openrouter/v1

